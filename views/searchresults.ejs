<!-- searchresults.ejs -->
<!DOCTYPE html>
<html>
<head>
  <title>Job Search Results</title>
</head>
<body>
  <h1>Job Search Results</h1>
  <ul>
    <% jobs.forEach((job) => { %>
      <li>
        <a href="/company/<%= job._id %>">
          <%= job.jobtitle %> - <%= job.company %>
        </a>
      </li>
    <% }) %>
  </ul>
</body>
</html>


